<div class="modal fade" id="modalLoginForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header text-center">
                <h4 class="modal-title w-100 font-weight-bold">LOGIN</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body mx-3">
                <form class="login">

                    <div class="register-switch row col-sm-10">
                        <input type="radio" name="user" value="dev" id="user-dev" class="register-switch-input" checked>
                        <label for="user-dev" class="register-switch-label">Developer</label>
                        <input type="radio" name="user" value="start" id="user-start" class="register-switch-input">
                        <label for="user-start" class="register-switch-label">Start-up</label>
                    </div>

                    <div class="md-form mb-2">
                        <div class="col-sm-10 input-icons p-0">
                            <i class="fas fa-at icon"></i>
                            <input type="user" class="form-control input-field validate" id="user-input"
                                placeholder="Email">
                        </div>
                    </div>

                    <div class="md-form mb-2">
                        <div class="col-sm-10 input-icons p-0">
                            <i class="fas fa-key icon"></i>
                            <input type="password" class="form-control input-field validate" id="pass-input"
                                placeholder="Password">
                        </div>
                    </div>

                    <div class="modal-footer d-flex justify-content-center">
                        <button type="submit" class="btn-grad">Login</button>
                    </div>
                </form>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>
<div class="container-fluid" id="developerContainerMain">
  <div class="row">
    <div class="col-sm-12">
      <div class="container-fluid">
        <div class="row">
          <div class="col profile">
            <div class="name">
              <h1>Hello, {{developer_name}}</h1>
            </div>
            <p>User Email: {{developer_email}}</p>
            <a class="navbar-brand" href="/user-logout">
              Logout
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>

    <!-- <div class="container-fluid title"> -->
    <!-- <h1 class="ml16">Available Jobs</h1> -->
    <div class="row">
      <!-- cj's working section -->
      <!-- contained the info so navbar could be added -->
      <div class="col-sm-3">

        <div class="container-fluid projDisp">
          <div class="btn-group" role="group" aria-label="Basic example">
            <button type="button" class="btn-proj btn-secondary" id="find">Find</button>
            <button type="button" class="btn-proj btn-secondary" id="active">Active</button>
            <button type="button" class="btn-proj btn-secondary" id="complete">Complete</button>
          </div>
          <div class="col-sm-12" id="projView">
            <ul id="findList">
              {{#each project}}
              <li class="project" data-id="{{id}}">
                <hi> {{title}} </hi>
                <p> $ {{price}} </p>
              </li>
              {{/each}}
            </ul>
            <ul id="activeList" style="display: none">
              {{#each activeProject}}
              <li class="project" data-id="{{id}}">
                <hi> {{title}} </hi>
                <p> {{price}} </p>
              </li>
              {{/each}}
            </ul>
            <ul id="completeList" style="display: none">
              {{#each completeProject}}
              <li class="project" data-id="{{id}}">
                <hi> {{title}} </hi>
                <p> {{price}} </p>
              </li>
              {{/each}}
            </ul>
          </div>

        </div>
      </div>
      <div class="p-0 col-sm-9 box" id="developer-display-section">

      </div>
    </div>
  </div>


</div>
